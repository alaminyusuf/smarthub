{
	"name": "graphql-api_server",
	"version": "1.0.0",
	"description": "An open Nodejs api using GraphQL",
	"main": "index.ts",
	"scripts": {
		"server": "nodemon src/index",
		"db:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n smarthub",
		"db:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n smarthub",
		"db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run"
	},
	"keywords": [
		"graphql",
		"nodejs",
		"express",
		"opensource"
	],
	"author": "Al-amin Yusuf<alaminyusuf131@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@types/connect-redis": "^0.0.17",
		"@types/cors": "^2.8.10",
		"@types/ioredis": "^4.27.7",
		"apollo-server-express": "^2.22.2",
		"argon2": "^0.28.2",
		"bcrypt": "^5.0.1",
		"bcryptjs": "^2.4.3",
		"connect-redis": "^6.0.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-session": "^1.17.1",
		"graphql": "^15.5.0",
		"ioredis": "^4.28.0",
		"pg": "^8.7.1",
		"reflect-metadata": "^0.1.13",
		"type-graphql": "^1.1.1",
		"typeorm": "^0.2.32"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.11",
		"@types/express-session": "^1.17.3",
		"@types/node": "^14.14.34",
		"nodemon": "^2.0.7",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3"
	}
}
